{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "func-names": "off",
    "dot-notation": "off",
    "no-undef": "off",
    "no-restricted-globals": "off",
    "no-useless-escape": "off",
    "eqeqeq": "off"
  }
}